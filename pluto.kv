<SpeedTest>:
    name: "SpeedTest"
    BoxLayout:
        orientation:"vertical"
        height:"40dp"
        size_hint_y:None
        BoxLayout:
            Button:
                text: "ButtonOne"
                size_hint_x:50
                on_press:
                    app.root.transition.direction = 'right'
                    app.root.current = 'Home'
            Button:
                text: 'Speed Test'
                size_hint_x:50
                on_press: root.test()


